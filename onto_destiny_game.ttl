@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix destiny: <http://destiny.game/ontology#> .

# Онтология игры Destiny
# Ontology of Destiny Game

# Классы онтологии
# Ontology Classes

destiny:GameEntity a owl:Class ;
    rdfs:subClassOf prov:Entity ;
    rdfs:label "Игровая сущность"@ru, 
        "Game Entity"@en ;
    rdfs:comment "Сущности игрового мира Destiny."@ru, "Entities of Destiny game world."@en .

destiny:Character a owl:Class ;
    rdfs:subClassOf prov:Person ;
    rdfs:label "Персонаж"@ru, 
        "Character"@en ;
    rdfs:comment "Персонажи игрового мира."@ru, "Characters of the game world."@en .

destiny:Location a owl:Class ;
    rdfs:subClassOf destiny:GameEntity ;
    rdfs:label "Локация"@ru, 
        "Location"@en ;
    rdfs:comment "Места в игровом мире."@ru, "Places in the game world."@en .

destiny:Object a owl:Class ;
    rdfs:subClassOf destiny:GameEntity ;
    rdfs:label "Объект"@ru, 
        "Object"@en ;
    rdfs:comment "Объекты игрового мира."@ru, "Objects of the game world."@en .

destiny:Organization a owl:Class ;
    rdfs:subClassOf prov:Organization ;
    rdfs:label "Организация"@ru, 
        "Organization"@en ;
    rdfs:comment "Организации и коллективы игрового мира."@ru, "Organizations and collectives of the game world."@en .

destiny:GameEvent a owl:Class ;
    rdfs:subClassOf prov:Activity ;
    rdfs:label "Игровое событие"@ru, 
        "Game Event"@en ;
    rdfs:comment "События в игровом мире."@ru, "Events in the game world."@en .

destiny:GameEventArc a owl:Class ;
    rdfs:subClassOf destiny:GameEvent ;
    rdfs:label "Арка игровых событий"@ru, 
        "Game Event Arc"@en ;
    rdfs:comment "Группа событий, объединенных сезоном или главой."@ru, "Group of events united by season or chapter."@en .

destiny:GeneralMotive a owl:Class ;
    rdfs:subClassOf prov:Plan ;
    rdfs:label "Общий мотив"@ru, 
        "General Motive"@en ;
    rdfs:comment "Общие мотивы, которые могут быть специализированы в конкретные планы."@ru, 
        "General motives that can be specialized into specific plans."@en .

# Отношения между классами
# Class relationships

destiny:containsEvent a owl:ObjectProperty ;
    rdfs:domain destiny:GameEventArc ;
    rdfs:range destiny:GameEvent ;
    rdfs:label "Содержит событие"@ru, 
        "Contains event"@en ;
    rdfs:comment "Отношение между аркой событий и событием, которое она содержит."@ru, "Relationship between event arc and event it contains."@en .

# Арки событий
# Event Arcs

destiny:collapse_arc a destiny:GameEventArc ;
    rdfs:label "Арка Коллапса"@ru, 
        "Collapse Arc"@en ;
    rdfs:comment "Катастрофическое событие, уничтожившее Золотой Век человечества."@ru, "Catastrophic event that destroyed humanity's Golden Age."@en ;
    prov:wasAssociatedWith destiny:traveler ;
    prov:atLocation destiny:solar_system ;
    destiny:containsEvent destiny:traveler_shard_loss .

destiny:red_war_arc a destiny:GameEventArc ;
    rdfs:label "Арка Красной войны"@ru, 
        "Red War Arc"@en ;
    rdfs:comment "Война между Кабал и человечеством, начавшаяся с нападения на Последний город."@ru, "War between the Cabal and humanity, beginning with the attack on the Last City."@en ;
    prov:wasAssociatedWith destiny:ghaul , destiny:cabal ;
    prov:wasInformedBy destiny:collapse_arc ;
    prov:atLocation destiny:earth ;
    prov:qualifiedAssociation [
        a prov:Association ;
        prov:agent destiny:ghaul ;
        prov:hadPlan destiny:conquer_traveler_light
    ] ;
    destiny:containsEvent destiny:ghaul_attacked_last_city , destiny:ghaul_deployed_almighty , 
        destiny:zavala_departed_to_titan , destiny:cayde_departed_to_nessus , destiny:ikora_departed_to_io , 
        destiny:guardians_arrived_at_farm , destiny:guardians_restored_light , destiny:edz_communication_network_was_setup , 
        destiny:zavala_signal_was_received , destiny:guardians_titan_arrival_reunion_occurred , destiny:guardians_retrieved_decryption_cpu , 
        destiny:sloane_decrypted_signal , destiny:guardians_arrived_nessus_search_cayde , destiny:guardians_gave_cayde_vex_teleporter , 
        destiny:guardians_reunited_with_ikora_on_io , destiny:red_legion_mined_traveler_energy_on_io , destiny:taken_invaded_io , 
        destiny:guardians_and_ikora_understood_ghaul_plan , destiny:guardians_accessed_warmind_vault , destiny:warmind_network_scan_revealed_almighty_danger , 
        destiny:ghaul_interrogated_speaker_and_learned_io_success , destiny:vanguard_reunited_at_farm , destiny:hawthorne_offered_infiltration_help , 
        destiny:vanguard_executed_counteroffensive_against_city , destiny:guardians_captured_thumos_ship_keys_for_almighty_approach , 
        destiny:guardians_stole_thumos_ship_and_departed_to_almighty , destiny:guardians_fought_and_disabled_almighty , 
        destiny:consul_and_speaker_were_killed , destiny:guardians_joined_counteroffensive_and_activated_teleporter , 
        destiny:guardians_defeated_ghaul , destiny:traveler_awakened_destroyed_ghaul_and_attracted_pyramids .

# События
# Events

destiny:traveler_shard_loss a destiny:GameEvent ;
    rdfs:label "Странник потерял осколок"@ru, 
        "Traveler lost shard"@en ;
    rdfs:comment "Во время Коллапса Странник потерял часть себя, которая упала на Землю в место, позже названное Европейской мертвой зоной."@ru, "During the Collapse, the Traveler lost a part of itself, which fell to Earth in a place later called the European Dead Zone."@en ;
    prov:wasAssociatedWith destiny:traveler ;
    prov:atLocation destiny:solar_system .

destiny:ghaul_attacked_last_city a destiny:GameEvent ;
    rdfs:label "Гоул напал на Последний город"@ru, 
        "Ghaul attacked the Last City"@en ;
    rdfs:comment "Гоул, командующий Красным легионом, напал на Последний город, захватил его и Глашатая, использовал устройство для лишения Стражей их Света."@ru, 
        "Ghaul, commander of the Red Legion, attacked the Last City, captured it and the Speaker, used a device to strip Guardians of their Light."@en ;
    prov:wasAssociatedWith destiny:ghaul , destiny:red_legion , destiny:guardians , destiny:traveler , destiny:speaker ;
    prov:atLocation destiny:last_city ;
    prov:used destiny:light_suppression_device ;
    prov:qualifiedAssociation [
        a prov:Association ;
        prov:agent destiny:ghaul ;
        prov:hadPlan destiny:conquer_traveler_light
    ] .

destiny:ghaul_deployed_almighty a destiny:GameEvent ;
    rdfs:label "Гоул разместил Всемогущий рядом с Солнцем"@ru, 
        "Ghaul deployed the Almighty near the Sun"@en ;
    rdfs:comment "Гоул разместил устройство Всемогущий рядом с Солнцем с целью уничтожить его, если Странник не поддастся его требованиям."@ru, 
        "Ghaul deployed the Almighty device near the Sun with the goal of destroying it if the Traveler does not submit to his demands."@en ;
    prov:wasAssociatedWith destiny:ghaul ;
    prov:wasInformedBy destiny:ghaul_attacked_last_city ;
    prov:atLocation destiny:solar_system ;
    prov:used destiny:almighty ;
    prov:qualifiedAssociation [
        a prov:Association ;
        prov:agent destiny:ghaul ;
        prov:hadPlan destiny:threaten_solar_system_destruction
    ] .

destiny:zavala_departed_to_titan a destiny:GameEvent ;
    rdfs:label "Завала отправился на Титан"@ru, 
        "Zavala departed to Titan"@en ;
    rdfs:comment "После падения Последнего города Завала отправился на Титан, чтобы найти убежище и собрать силы."@ru, 
        "After the fall of the Last City, Zavala departed to Titan to find refuge and gather forces."@en ;
    prov:wasAssociatedWith destiny:zavala ;
    prov:wasInformedBy destiny:ghaul_attacked_last_city ;
    prov:atLocation destiny:titan ;
    prov:qualifiedAssociation [
        a prov:Association ;
        prov:agent destiny:zavala ;
        prov:hadPlan destiny:gather_forces_on_titan
    ] .

destiny:cayde_departed_to_nessus a destiny:GameEvent ;
    rdfs:label "Кейд отправился на Несс"@ru, 
        "Cayde departed to Nessus"@en ;
    rdfs:comment "После падения Последнего города Кейд отправился на Несс."@ru, 
        "After the fall of the Last City, Cayde departed to Nessus."@en ;
    prov:wasAssociatedWith destiny:cayde ;
    prov:wasInformedBy destiny:ghaul_attacked_last_city ;
    prov:atLocation destiny:nessus .

destiny:ikora_departed_to_io a destiny:GameEvent ;
    rdfs:label "Икора отправилась на Ио"@ru, 
        "Ikora departed to Io"@en ;
    rdfs:comment "После падения Последнего города Икора отправилась на Ио в поисках ответов, так как это последнее место, где был Странник."@ru, 
        "After the fall of the Last City, Ikora departed to Io seeking answers, as this is the last place the Traveler touched."@en ;
    prov:wasAssociatedWith destiny:ikora ;
    prov:wasInformedBy destiny:ghaul_attacked_last_city ;
    prov:atLocation destiny:io ;
    prov:qualifiedAssociation [
        a prov:Association ;
        prov:agent destiny:ikora ;
        prov:hadPlan destiny:seek_traveler_answers_on_io
    ] .

destiny:guardians_reunited_with_ikora_on_io a destiny:GameEvent ;
    rdfs:label "Стражи воссоединились с Икорой на Ио"@ru, 
        "Guardians reunited with Ikora on Io"@en ;
    rdfs:comment "Стражи нашли Икору на Ио в Потерянном оазисе, последнем месте, где был Странник. Икора рассказала о том, что Красный легион ищет что-то, что они никогда не смогут понять."@ru, 
        "Guardians found Ikora on Io in the Lost Oasis, the last place the Traveler touched. Ikora told them that the Red Legion is searching for something they could never possibly understand."@en ;
    prov:wasAssociatedWith destiny:guardians , destiny:ikora ;
    prov:wasInformedBy destiny:guardians_gave_cayde_vex_teleporter ;
    prov:atLocation destiny:io ;
    prov:qualifiedAssociation [
        a prov:Association ;
        prov:agent destiny:guardians ;
        prov:hadPlan destiny:complete_vanguard_reunion
    ] .

destiny:red_legion_mined_traveler_energy_on_io a destiny:GameEvent ;
    rdfs:label "Красный легион добыл энергию Странника на Ио"@ru, 
        "Red Legion mined Traveler's energy on Io"@en ;
    rdfs:comment "Красный легион добыл остаточную энергию Странника на Ио, пытаясь трансформировать себя в носителей Света, как Орикс трансформировал Одержимых с помощью темных сил."@ru, 
        "The Red Legion mined the Traveler's remnant energy on Io, attempting to transform themselves into wielders of Light, as Oryx transformed the Taken with dark powers."@en ;
    prov:wasAssociatedWith destiny:red_legion , destiny:ghaul ;
    prov:wasInformedBy destiny:ghaul_attacked_last_city ;
    prov:atLocation destiny:io ;
    prov:qualifiedAssociation [
        a prov:Association ;
        prov:agent destiny:ghaul ;
        prov:hadPlan destiny:transform_legion_with_light
    ] .

destiny:taken_invaded_io a destiny:GameEvent ;
    rdfs:label "Одержимые вторглись на Ио"@ru, 
        "Taken invasion of Io"@en ;
    rdfs:comment "Одержимые привлечены энергией Странника на Ио, раскопанной Красным легионом, и создают порталы для призыва своих сил."@ru, 
        "The Taken are drawn to the Traveler's energy on Io, mined by the Red Legion, and are creating portals to summon their forces."@en ;
    prov:wasAssociatedWith destiny:taken ;
    prov:wasInformedBy destiny:red_legion_mined_traveler_energy_on_io ;
    prov:atLocation destiny:io ;
    prov:qualifiedAssociation [
        a prov:Association ;
        prov:agent destiny:taken ;
        prov:hadPlan destiny:consume_light_on_io
    ] .

destiny:guardians_and_ikora_understood_ghaul_plan a destiny:GameEvent ;
    rdfs:label "Стражи и Икора поняли план Гоула о трансформации себя и Красного легиона в носителей Света"@ru, 
        "Guardians and Ikora understood Ghaul's plan about transforming themselves and the Red Legion into wielders of Light"@en ;
    rdfs:comment "Стражи и Икора поняли, что Гоул добывает остаточную энергию Странника на Ио в надежде трансформировать себя и Красный легион в носителей Света, как Орикс трансформировал Одержимых с помощью темных сил."@ru, 
        "Guardians and Ikora understood that Ghaul is mining the Traveler's remnant energy on Io in hopes of transforming himself and the Red Legion into wielders of Light, as Oryx transformed the Taken with dark powers."@en ;
    prov:wasAssociatedWith destiny:guardians , destiny:ikora ;
    prov:wasInformedBy destiny:red_legion_mined_traveler_energy_on_io , destiny:taken_invaded_io , destiny:guardians_reunited_with_ikora_on_io ;
    prov:atLocation destiny:io ;
    prov:used destiny:transform_legion_with_light .

destiny:guardians_accessed_warmind_vault a destiny:GameEvent ;
    rdfs:label "Стражи получили доступ к Военному Разуму"@ru, 
        "Guardians accessed Warmind"@en ;
    rdfs:comment "Стражи проникли к Военному Разуму на Ио через туннель, открытый буром Красного легиона, чтобы использовать сканер для анализа Всемогущего."@ru, 
        "Guardians accessed the Warmind on Io through a tunnel exposed by the Red Legion drill to use the scanner for analyzing the Almighty."@en ;
    prov:wasAssociatedWith destiny:guardians , destiny:asher_mir, destiny:ikora , destiny:zavala ;
    prov:wasInformedBy destiny:guardians_and_ikora_understood_ghaul_plan ;
    prov:atLocation destiny:io ;
    prov:used destiny:warmind ;
    prov:qualifiedAssociation [
        a prov:Association ;
        prov:agent destiny:asher_mir ;
        prov:hadPlan destiny:discredit_zavala_plan
    ] ,
    [
        a prov:Association ;
        prov:agent destiny:zavala ;
        prov:hadPlan destiny:attack_almighty_with_warmind_data
    ] .

destiny:warmind_network_scan_revealed_almighty_danger a destiny:GameEvent ;
    rdfs:label "Сканирование сети Военного Разума выявило опасность Всемогущего"@ru, 
        "Warmind network scan revealed Almighty danger"@en ;
    rdfs:comment "Сканирование Военного Разума показало, что Всемогущий неразрывно связан с гармоническим резонансом магнитных потоковых трубок Солнца. Если Всемогущий будет уничтожен, то это уничтожит Солнце."@ru, 
        "The Warmind scan revealed that the Almighty is inextricably bound to the harmonic resonance of the sun's magnetic flux tubes. If the Almighty is destroyed, it will take the sun with it."@en ;
    prov:wasAssociatedWith destiny:guardians , destiny:asher_mir , destiny:vanguard ;
    prov:wasInformedBy destiny:guardians_accessed_warmind_vault ;
    prov:atLocation destiny:io ;
    prov:used destiny:warmind , destiny:almighty .

destiny:ghaul_interrogated_speaker_and_learned_io_success a destiny:GameEvent ;
    rdfs:label "Гоул допрашивал Глашатая о Свете и узнал об успехе операции на Ио"@ru, 
        "Ghaul interrogated Speaker about Light and learned of Io operation success"@en ;
    rdfs:comment "Гоул допрашивал Глашатая, спрашивая, что делает Стражей достойными Света. Глашатай ответил: 'Преданность. Самопожертвование. Смерть.' Также Гоул узнал от Консула, что операция на Ио прошла успешно и клетка почти завершена."@ru, 
        "Ghaul interrogated the Speaker, asking what makes Guardians worthy of the Light. The Speaker answered: 'Devotion. Self-Sacrifice. Death.' Ghaul also learned from the Consul that the Io operation was successful and the cage is nearly complete."@en ;
    prov:wasAssociatedWith destiny:ghaul , destiny:speaker , destiny:consul ;
    prov:wasInformedBy destiny:red_legion_mined_traveler_energy_on_io ;
    prov:atLocation destiny:the_immortal ;
    prov:qualifiedAssociation [
        a prov:Association ;
        prov:agent destiny:ghaul ;
        prov:hadPlan destiny:conquer_traveler_light
    ] .

destiny:vanguard_reunited_at_farm a destiny:GameEvent ;
    rdfs:label "Авангард воссоединился на Ферме"@ru, 
        "Vanguard reunited at the Farm"@en ;
    rdfs:comment "Завала, Икора и Кейд воссоединились на Ферме после долгого разделения. Они обсудили угрозу Всемогущего и необходимость освобождения Странника."@ru, 
        "Zavala, Ikora and Cayde reunited at the Farm after a long separation. They discussed the threat of the Almighty and the need to free the Traveler."@en ;
    prov:wasAssociatedWith destiny:vanguard , destiny:guardians ;
    prov:wasInformedBy destiny:warmind_network_scan_revealed_almighty_danger , destiny:guardians_gave_cayde_vex_teleporter ;
    prov:atLocation destiny:the_farm ;
    prov:qualifiedAssociation [
        a prov:Association ;
        prov:agent destiny:vanguard ;
        prov:hadPlan destiny:disable_almighty_instead_of_destroy
    ] .

destiny:hawthorne_offered_infiltration_help a destiny:GameEvent ;
    rdfs:label "Хоторн предложила помощь в проникновении в город"@ru, 
        "Hawthorne offered infiltration help"@en ;
    rdfs:comment "Хоторн предложила помочь Стражам проникнуть в город незаметно, используя свои знания о стенах города и навыки скрытного проникновения."@ru, 
        "Hawthorne offered to help Guardians infiltrate the city unnoticed, using her knowledge of city walls and stealth infiltration skills."@en ;
    prov:wasAssociatedWith destiny:hawthorne, destiny:vanguard , destiny:guardians ;
    prov:wasInformedBy destiny:vanguard_reunited_at_farm ;
    prov:atLocation destiny:the_farm ;
    prov:qualifiedAssociation [
        a prov:Association ;
        prov:agent destiny:hawthorne ;
        prov:hadPlan destiny:help_guardians_infiltrate_city
    ] .

destiny:vanguard_executed_counteroffensive_against_city a destiny:GameEvent ;
    rdfs:label "Авангард осуществил контрнаступление против города"@ru, 
        "Vanguard executed counteroffensive against the city"@en ;
    rdfs:comment "Авангард возглавил армию Стражей без Света в контрнаступлении против оккупированного города, надеясь освободить Странника."@ru, 
        "The Vanguard led a host of Lightless Guardians in a counteroffensive against the occupied City, hoping to free the Traveler."@en ;
    prov:wasAssociatedWith destiny:hawthorne , destiny:vanguard ;
    prov:wasInformedBy destiny:hawthorne_offered_infiltration_help , destiny:guardians_fought_and_disabled_almighty ;
    prov:atLocation destiny:last_city ;
    prov:qualifiedAssociation [
        a prov:Association ;
        prov:agent destiny:vanguard, destiny:hawthorne ;
        prov:hadPlan destiny:free_occupied_city
    ] .

destiny:guardians_captured_thumos_ship_keys_for_almighty_approach a destiny:GameEvent ;
    rdfs:label "Стражи захватили ключи доступа к кораблю Тумоса, чтобы незаметно подобраться к Всемогущему"@ru, 
        "Guardians captured Thumos ship keys to approach Almighty unnoticed"@en ;
    rdfs:comment "Стражи получили задание отключить супероружие Всемогущий и для этого должны были украсть корабль Тумоса Несломленного. Аманда Холлидей доставила им танк, с помощью которого они штурмовали объект Кабал, проникли на борт корабля Эхион, победили Тумоса и получили ключи доступа к его кораблю для незаметного приближения к Всемогущему."@ru, 
        "Guardians were tasked to disable the Almighty superweapon and for this had to steal the ship of Thumos the Unbroken. Amanda Holliday delivered them a tank, with which they stormed the Cabal facility, infiltrated the Echion ship, defeated Thumos and obtained key codes to his ship for undetected approach to the Almighty."@en ;
    prov:wasAssociatedWith destiny:guardians , destiny:thumos_unbroken , destiny:amanda_holliday , 
        destiny:red_legion , destiny:hawthorne ;
    prov:wasInformedBy destiny:vanguard_reunited_at_farm ;
    prov:atLocation destiny:european_dead_zone ;
    prov:used destiny:thumos_key_codes ;
    prov:qualifiedAssociation [
        a prov:Association ;
        prov:agent destiny:guardians ;
        prov:hadPlan destiny:disable_almighty_instead_of_destroy
    ] ,
    [
        a prov:Association ;
        prov:agent destiny:amanda_holliday ;
        prov:hadPlan destiny:provide_tank_and_air_support
    ] ,
    [
        a prov:Association ;
        prov:agent destiny:hawthorne ;
        prov:hadPlan destiny:revenge_for_killed_team
    ] .

destiny:guardians_stole_thumos_ship_and_departed_to_almighty a destiny:GameEvent ;
    rdfs:label "Стражи угнали корабль Тумоса и отправились на Всемогущий"@ru, 
        "Guardians stole Thumos's ship and departed to the Almighty"@en ;
    rdfs:comment "Стражи использовали украденные ключи доступа для захвата корабля Тумоса Несломленного и отправились на Всемогущий для отключения Всемогущего."@ru, 
        "Guardians used the stolen key codes to hijack the ship of Thumos the Unbroken and departed to the Almighty to disable the Almighty."@en ;
    prov:wasAssociatedWith destiny:guardians , destiny:red_legion ;
    prov:wasInformedBy destiny:guardians_captured_thumos_ship_keys_for_almighty_approach ;
    prov:atLocation destiny:european_dead_zone ;
    prov:used destiny:thumos_key_codes , destiny:thumos_ship ;
    prov:qualifiedAssociation [
        a prov:Association ;
        prov:agent destiny:guardians ;
        prov:hadPlan destiny:disable_almighty_instead_of_destroy
    ] .

destiny:guardians_fought_and_disabled_almighty a destiny:GameEvent ;
    rdfs:label "Стражи с боем отключили Всемогущего"@ru, 
        "Guardians fought and disabled the Almighty"@en ;
    rdfs:comment "Стражи проникли на Всемогущий, обнаружили, что Меркурий используется как топливо, уничтожили тепловые обменники и перегрузили электронный резервуар, полностью отключив оружие Всемогущего."@ru, 
        "Guardians infiltrated the Almighty, discovered that Mercury was being used as fuel, destroyed thermal exchangers and overloaded the electron reservoir, completely disabling the Almighty's weapon."@en ;
    prov:wasAssociatedWith destiny:guardians , destiny:red_legion ;
    prov:wasInformedBy destiny:guardians_stole_thumos_ship_and_departed_to_almighty ;
    prov:atLocation destiny:almighty ;
    prov:used destiny:almighty ;
    prov:qualifiedAssociation [
        a prov:Association ;
        prov:agent destiny:guardians ;
